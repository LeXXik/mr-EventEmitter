  "bugs": {
    "url": "https://github.com/Maksims/mr-EventEmitter/issues"
  },
  "homepage": "https://github.com/Maksims/mr-EventEmitter#readme",
  "dependencies": {},
  "devDependencies": {
    "google-closure-compiler": "^20201207.0.0",
    "jsdoc-to-markdown": "^6.0.1"
  }
}
